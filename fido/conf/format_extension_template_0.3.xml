<?xml version="1.0" encoding="UTF-8"?>
<!-- Looking at these for usage of DC:
  http://dublincore.org/documents/usageguide/elements.shtml
  http://dublincore.org/documents/usageguide/qualifiers.shtml
  http://www.dublincore.org/documents/dc-xml-guidelines/
-->
<formats 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  version="0.3" 
  xsi:noNamespaceSchemaLocation="fido-formats.xsd" 
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:dcterms="http://purl.org/dc/terms/">
  <format>
    <puid>fido-fmt/189.word</puid>
    <name>Microsoft Office Open XML - Word</name>
    <version>1.0 Transitional</version>
    <alias>Word OOXML</alias>
    <description></description>
    <!-- release date -->
    <dcterms:available/>
    <content-type>raster|vector|image|spreadsheet|text|wordprocessed|page_description|audio|presentation|mark-up|database|video|email</content-type>
    <extension>docx</extension>
    <apple_uid/>
    <has_priority_over>x-fmt/263</has_priority_over>
    <has_priority_over>fmt/189</has_priority_over>
    <is_subtype_of/>
    <dc:creator>Microsoft</dc:creator>
    <reference type="information|specification|implementation|registry|related">
      <dc:title/>
      <dc:creator/>
      <dc:publisher/>
      <dc:created/>
      <dc:modified/>
      <dc:available/>
      <note/>
    </reference>
    <record status="invalid|incomplete|stub|adequate|complete|superb|deprecated">
      <dcterms:isReplacedBy/>
      <dcterms:Replaces/>
      <dc:creator/>
      <dc:provenance/>
      <dcterms:licence/>
      <dc:date/> creation and update?
      <dc:source/> re-use identifier from references.
    </record>
    <example_file>
      <dc:identifier>./this.txt</dc:identifier>
      <checksum type="md5"/>
      <dc:rights>To the extent possible under law, Andrew Jackson has waived all copyright and related or neighboring rights to this example file.</dc:rights>
      <dcterms:license>http://creativecommons.org/publicdomain/zero/1.0/</dcterms:license>
    </example_file>
    <signature>
      <name>Microsoft Office Open XML - Word</name>
      <pattern>
        <position>BOF</position>
        <regex>(?s)\APK\x03\x04</regex>
      </pattern>
      <pattern>
        <position>BOF</position>
        <regex>(?s)\A.{30}\[Content_Types\]\.xml \xa2</regex>
      </pattern>
      <pattern>
        <position>EOF</position>
        <regex>(?s)\x00\x00word/.{1,20}\.xmlPK\x01\x02\x2d.{0,2000}\Z</regex>
      </pattern>
    </signature>
  </format>
</formats>
